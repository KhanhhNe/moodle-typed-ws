{
  "name": "moodle-typed-ws",
  "description": "A fully typed Moodle WebService client library.",
  "author": {
    "name": "KhanhhNe",
    "url": "https://github.com/KhanhhNe"
  },
  "version": "0.1.0",
  "engines.node": ">=18.0.0",
  "sideEffects": false,
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist ; bun tsc -d --outdir dist",
    "clean": "rm -rf dist src/data",
    "clone": "(cd src/moodle-docker && bash copy-types.sh)",
    "parse": "bun src/parse/index.ts",
    "format": "bun eslint --fix . && prettier --write .",
    "prepublish": "bun clone && bun parse && bun run build",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "object-to-formdata": "^4.5.1"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.2",
    "@types/node": "^18.16.0",
    "@types/ramda": "^0.29.4",
    "@types/rollup": "^0.54.0",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "bun-types": "^1.0.2",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.13",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "npm:eslint-plugin-i@2.28.0-2",
    "json-schema-to-typescript": "^13.1.1",
    "php-parser": "^3.1.5",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "ramda": "^0.29.0",
    "typescript": "^5.1.6",
    "zod": "^3.22.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.19.0"
  },
  "keywords": [
    "moodle",
    "moodle web service",
    "moodle client",
    "moodle ws",
    "typescript"
  ]
}