{
  "name": "moodle-typed-ws",
  "type": "module",
  "version": "0.1.9",
  "description": "A fully typed Moodle WebService client library.",
  "author": {
    "name": "KhanhhNe",
    "url": "https://github.com/KhanhhNe"
  },
  "homepage": "https://github.com/KhanhhNe/moodle-typed-ws",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KhanhhNe/moodle-typed-ws.git"
  },
  "engines.node": ">=18.0.0",
  "keywords": [
    "moodle",
    "moodle web service",
    "moodle client",
    "moodle ws",
    "typescript"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "rm -rf dist ; bun tsup src/index.ts --dts --format cjs,esm",
    "clean": "rm -rf dist src/data",
    "clone": "(cd src/moodle-docker && bash copy-types.sh)",
    "parse": "bun src/parse/index.ts",
    "format": "bun eslint --fix .",
    "prepublish": "bun clone && bun parse && bun run build",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "object-to-formdata": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.6",
    "@types/node": "^18.16.0",
    "@types/ramda": "^0.29.4",
    "bun-types": "^1.0.2",
    "eslint": "^8.47.0",
    "json-schema-to-typescript": "^13.1.1",
    "php-parser": "^3.1.5",
    "prettier": "^3.0.0",
    "ramda": "^0.29.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "zod": "^3.22.2"
  },
  "ct3aMetadata": {
    "initVersion": "7.19.0"
  }
}
